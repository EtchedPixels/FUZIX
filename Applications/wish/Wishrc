# Here are some basic aliases and stuff for Wish 2.0
#
# setenv PATH $HOME/.bin:/usr/local/bin:/usr/etc:/usr/ucb:/bin:/usr/bin
set dir ' '
set prompt '%d: '
#
alias , 'make $*'
alias boring 'echo I know'
alias h history
alias j jobs
alias logo exit
#alias ls '/bin/ls -lg $*'
#alias l '/bin/ls -C $*'
alias mn 'nroff -man $* | less'
alias zless 'zcat $* | less'
